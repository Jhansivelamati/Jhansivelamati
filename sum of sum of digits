#include<stdio.h>
#include<conio.h>
int digit(int);
int digit(int n)
{
    int res=0;
    while(n>0)
    {
        res=res+n%10;
        n=n/10;
    }
    return res;
}
void main()
{
    int a[10]={10,20,30,40,50,60,70,80,90,10};
    int sum=0;
    for(int i=0;i<10;i++)
    {
        sum=sum+digit(a[i]);
    }
        printf("The key is:%d",sum);

}
